# include <stdio.h>
int main()
{
    int a[50],n,i,large,sm;
    printf("Enter the number of elements: ");
    scanf("%d",&n);
    printf("\nEnter the array elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    large=0,sm=0;
    for(i=0;i<n;i++)
    {
        if(a[i]>large)
        {
            sm=large;
            large=a[i];
        }
    }
    printf("\nLargest is: %d",large);
    printf("\nSecond largest is: %d",sm);
return 0;
}
