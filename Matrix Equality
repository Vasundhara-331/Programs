# include <stdio.h>
void read(int [][10],int,int);
void print(int [][10],int,int);
void equal(int [][10],int [][10],int,int);
void main()
{
    int a[10][10], b[10][10],r,c;
    printf("Enter the order of the matrices:\n");
    scanf("%d%d",&r,&c);
    printf("\nEnter the elements of matrix A:\n");
    read(a,r,c);
    printf("\nEnter the elements of matrix B:\n");
    read(b,r,c);
    printf("\nMatrix A:\n");
    print(a,r,c);
    printf("\nMatrix B:\n");
    print(b,r,c);
    equal(a,b,r,c);
}
void read(int m[][10],int r1, int c1)
{
    int i,j;
    for(i=0;i<r1;i++)
    {
        for(j=0;j<c1;j++)
        {
            scanf("%d",&m[i][j]);
        }
    }
}
void print(int m[][10],int r1, int c1)
{
    int i,j;
    for(i=0;i<r1;i++)
    {
        for(j=0;j<c1;j++)
        {
            printf("%d\t",m[i][j]);
        }
        printf("\n");
    }
}
void equal(int a1[][10], int b1[][10], int r1, int c1)
{
    int i,j,count=0;
    for(i=0;i<r1;i++)
    {
        for(j=0;j<c1;j++)
        {
            if(a1[i][j]==b1[i][j])
            count++;
        }
    }
    if(count==(r1*c1))
    printf("\nMatrices are equal");
    else
    printf("\nMatrices are not equal");
}
