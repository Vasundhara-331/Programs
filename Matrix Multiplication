# include <stdio.h>
int main()
{
   int a[10][10], b[10][10], p[10][10],i,j,k,r1,c1,r2,c2;
   printf("For matrix A:");
   printf("\n");
   printf("\nEnter the number of rows:");
   scanf("%d",&r1);
   printf("\nEnter the number of columns:");
   scanf("%d",&c1);
   printf("\nFor matrix B:");
   printf("\n");
   printf("\nEnter the number of rows:");
   scanf("%d",&r2);
   printf("\nEnter the number of columns:");
   scanf("%d",&c2);
    if(c1!=r2)
    {
       printf("\nMatrix multiplication not possible!!");
    }
    else
    {
       printf("Reading matrix A:");
       printf("\n");
        for(i=0;i<r1;i++)
       {
         for(j=0;j<c1;j++)
         {
            printf("\nEnter A[%d][%d]:",i,j);
            printf("\n");
            scanf("%d",&a[i][j]);
         }
    }
     printf("\nReading matrix B:");
     printf("\n");
     for(i=0;i<r2;i++)
     {
       for(j=0;j<c2;j++)
       {
         printf("\nEnter B[%d][%d]:",i,j);
         printf("\n");
         scanf("%d",&b[i][j]);
       }
     }
      printf("\nPrinting elements of A:");
      printf("\n");
      for(i=0;i<r1;i++)
      {
        for(j=0;j<c1;j++)
          {
             printf("%d\t",a[i][j]);
          }
          printf("\n");
      }
        printf("\nPrinting elements of B:");
        printf("\n");
        for(i=0;i<r2;i++)
        {
          for(j=0;j<c2;j++)
          {
             printf("%d\t",b[i][j]);
          }
          printf("\n");
        }
        for(i=0;i<r1;i++)
        {
          for(j=0;j<c2;j++)
          {
            for(k=0;k<c1;k++)
            {
               p[i][j]=p[i][j]+(a[i][k]*b[k][j]);
            }
          }
        }
        printf("\nProduct matrix:");
        printf("\n");
        for(i=0;i<r1;i++)
        {
          for(j=0;j<c2;j++)
          {
             printf("%d\t",p[i][j]);
          }
          printf("\n");
        }
  }
return 0;
}

