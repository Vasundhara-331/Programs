# include <stdio.h>
int main()
{
    char str[50];
    int i,v=0,c=0,space=0,d=0,special=0;
    printf("Enter a string ending with '$' symbol:");
    printf("\n");
    scanf("%[^$]s",str);
    for(i=0;str[i]!='\0';i++)
    {
        if((str[i]>='A') && (str[i]<='Z') || (str[i]>='a') &&(str[i]<='z'))
        {
        if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')
            v++;
        else
            c++;
        }
        else if(str[i]==' ')
        space++;
        else if(str[i]>='0' && str[i]<='9')
        d++;
        else
        special++;
    }
    printf("\nNumber of vowels is:%d",v);
    printf("\nNumber of consonants is:%d",c);
    printf("\nNumber of spaces is:%d",space);
    printf("\nNumber of digits is:%d",d);
    printf("\nNumber of special characters is:%d",special);
    return 0;
}
