# include <stdio.h>
int swap( int *, int *);
int main()
{
    int a=3, b=4;
    printf("a and b before swapping: %d and %d",a,b);
    swap(&a,&b);
    printf("\na and b after swapping: %d and %d",a,b);
}
int swap (int *m, int *n)
{
    printf("\nm and n before swapping: %d and %d",*m,*n);
    *m=*m+*n;
    *n=*m-*n;
    *m=*m-*n;
    printf("\nm and n after swappinng: %d and %d",*m,*n);
}
