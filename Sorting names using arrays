# include <stdio.h>
# include <string.h>
int main()
{
    char name [10][10], tname[10][10], temp[10];
    int i,j,n;
    printf("Enter the number of names:\n");
    scanf("%d",&n);
    printf("\nEnter %d names: \n",n);
    for(i=0;i<n;i++)
    {
        scanf("%s",name[i]);
        strcpy(tname[i],name[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(strcmp(name[i],name[j])>0)
            {
                strcpy(temp,name[i]);
                strcpy(name[i],name[j]);
                strcpy(name[j],temp);
            }
        }
    }
    printf("\nOriginal vs Sorted is:\n");
    for(i=0;i<n;i++)
    {
        printf("%s\t\t%s\n",tname[i],name[i]);
    }
    return 0;
}
