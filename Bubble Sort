#include <stdio.h>
int main () 
{
int n, a[20], i, j, k, t;
printf ("Enter the limit of the array:");
scanf ("%d", &n);
printf ("Enter the elments of the array:");
for (i = 0; i < n; i++)
    {
      scanf ("%d", &a[i]);
    }
for (i = 0; i < n - 1; i++)
    {
      for (j = 0; j < n - i - 1; j++)
    	{	  
        if (a[j] > a[j + 1])
	      {     
          t = a[j];	      
          a[j] = a[j + 1];	      
          a[j + 1] = t;
        }
      }
printf ("\nRound %d: ", i + 1);
for (k = 0; k < n; k++)
	{
printf ("%d\t", a[k]);
  }
   }
return 0;
}
