# include <stdio.h>
int main()
{
    int a[10][10],t[20][20],i,j,m,n;
    printf("\nEnter the number of rows and columns:");
    scanf("%d%d",&m,&n);
    printf("Enter the elements of matrix A:\n");
    for(i=0;i<m;i++)
    {
        for(j=0;j<n;j++)
        {
           printf("a[%d][%d]: ",m,n);
           scanf("%d",&a[i][j]); 
        }
        printf("\n");
    }
    printf("Transpose is:\n");
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
            t[i][j]=a[j][i];
            printf("%d\t",t[i][j]);
        }
        printf("\n");
    }
    return 0;
}
